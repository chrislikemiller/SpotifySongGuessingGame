  </ItemGroup>

  <ItemGroup>
    <Page Update="ManualReleaseDateCorrectionView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
